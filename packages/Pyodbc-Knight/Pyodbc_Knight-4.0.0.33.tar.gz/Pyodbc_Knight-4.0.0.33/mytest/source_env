[ -f ~/.bashrc ] && . ~/.bashrc
GBASEDBTDIR=/opt/GBASE/gbase
GBASEDBTSERVER=ol_gbasedbt10
ONCONFIG=onconfig.$GBASEDBTSERVER
GBASEDBTSQLHOSTS=/opt/GBASE/gbase/etc/sqlhosts.$GBASEDBTSERVER
GL_USEGLU=1
PSORT_DBTEMP=/tmp
PATH=${GBASEDBTDIR}/bin:${GBASEDBTDIR}/extend/krakatoa/jre/bin:${PATH}
LD_LIBRARY_PATH=$GBASEDBTDIR/lib:$GBASEDBTDIR/lib/esql:$GBASEDBTDIR/lib/c++:$GBASEDBTDIR/lib/dmi:$GBASEDBTDIR/lib/cli:$LD_LIBRARY_PATH
IFX_DISABLE_IPV6=yes
GL_DATE="%m%d%iY"
GL_DATETIME="%iY-%m-%d %H:%M:%S"
DB_LOCALE=zh_cn.utf8
CLIENT_LOCALE=zh_cn.utf8
SERVER_LOCALE=zh_cn.utf8
TERM=vt100
TREMCAP=$GBASEDBTDIR/etc/termcap
GBASEDBTTERM=terminfo
ODBCINI=${PWD}/odbc.ini
ODBCSYSINI=${PWD}
export GBASEDBTDIR GBASEDBTSERVER ONCONFIG GBASEDBTSQLHOSTS GL_USEGLU PATH LD_LIBRARY_PATH IFX_DISABLE_IPV6 GL_DATE GL_DATETIME DB_LOCALE CLIENT_LOCALE SERVER_LOCALE TREMCAP GBASEDBTTERM ODBCINI ODBCSYSINI

#export GBASEDBTSERVER=ol_gbasedbt10_ssl

