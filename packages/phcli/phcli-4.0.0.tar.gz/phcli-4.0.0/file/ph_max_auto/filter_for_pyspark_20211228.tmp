    #  Filter Start
    kwargs["filter_kvs"] = #filter_kvs#
    filter_kvs = kwargs["filter_kvs"]  # array [{"key", ["opt", "val"]}]
    for item in filter_kvs:
        key = list(item.keys())[0]
        opt, val = list(item.values())[0]
        data_frame = data_frame.filter(f"`{key}` {opt} '{val}'")
    #  Filter End
data_frame

