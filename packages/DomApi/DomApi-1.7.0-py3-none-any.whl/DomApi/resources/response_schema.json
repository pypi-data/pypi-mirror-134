{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "eventAt": {
      "type": "string",
      "pattern": "^(\\d{4}-\\d{2}-\\d{2}T(\\d{2}:){2}\\d{2}\\.\\d{3})"
    },
    "storeOrders": {
      "type": "array",
      "items": { "$ref": "#/$defs/order" },
      "minitems":1,
      "uniqueItems": true
    }
  },
  "required": [
    "eventAt",
    "storeState",
    "storeOrders",
    "storeEmployees"
  ],
  "$defs":{
    "order":{
      "type": "object",
      "properties": {
            "orderId": {
              "type": "integer"
            },
            "orderPlaced": {
              "type": "string",
              "pattern": "^(\\d{4}-\\d{2}-\\d{2}T(\\d{2}:){2}\\d{2}\\.\\d{3})"
            },
            "orderReady": {
              "type": "string",
              "pattern": "^(\\d{4}-\\d{2}-\\d{2}T(\\d{2}:){2}\\d{2}\\.\\d{3})"
            },
            "orderMakeSeconds": {
              "type": "integer"
            }            
          },
          "required": [
            "orderId",
            "orderPlaced",
            "orderReady",
            "orderMakeSeconds"
          ]
      }
  }
}